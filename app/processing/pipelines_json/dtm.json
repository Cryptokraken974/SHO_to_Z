{
  "pipeline": [
    "input/default.laz",
    {
      "type": "filters.outlier",
      "method": "statistical",
      "multiplier": 3,
      "mean_k": 8
    },
    {
      "type": "filters.assign",
      "assignment": "Classification[:]=0"
    },
    {
        "type": "filters.csf",
        "ignore": "Classification[7:7]",
        "slope": 0.15,
        "class_threshold": 0.35,
        "cloth_resolution": "__CLOTH_RESOLUTION__",
        "rigidness": 1,
        "iterations": 500,
        "points":true
    },
    {
      "type": "filters.range",
      "limits": "Classification![135:146],Z[-10:3000]"
    },
    {
      "type": "filters.range",
      "limits": "Classification[2:2]"
    },
    {
      "type": "writers.gdal",
      "filename": "output/default_DTM.tif",
      "resolution": "__RESOLUTION__",
      "output_type": "min",
      "nodata": -9999,
      "gdaldriver": "GTiff"
    }
  ]
}

# OpenTopography API Credentials
# Sign up at: https://portal.opentopography.org/
# Copy this file to .env and add your actual credentials

# Option 1: Use API Key (recommended)
OPENTOPO_API_KEY=your_api_key_here

# Option 2: Use Username/Password
OPENTOPO_USERNAME=your_username_here
OPENTOPO_PASSWORD=your_password_here

# Optional: Override download settings
ELEVATION_BBOX_BUFFER=0.01
ELEVATION_TIMEOUT=300
ELEVATION_MAX_RETRIES=3

# Optional: Database and other service credentials
DATABASE_URL=postgresql://user:password@localhost/dbname
REDIS_URL=redis://localhost:6379
